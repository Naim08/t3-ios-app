{
  "expo": {
    "name": "Pocket T3",
    "slug": "pocket-t3",
    "scheme": "pocket-t3",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "privacy": "unlisted",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.name.pockett3",
      "buildNumber": "3",
      "requireFullScreen": false,
      "userInterfaceStyle": "automatic",        "infoPlist": {
        "CFBundleDisplayName": "GPT Claude T3 Gemini Chat Bot",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": false,
          "NSExceptionDomains": {
            "buy.itunes.apple.com": {
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSExceptionMinimumTLSVersion": "1.0",
              "NSIncludesSubdomains": true
            },
            "sandbox.itunes.apple.com": {
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSExceptionMinimumTLSVersion": "1.0",
              "NSIncludesSubdomains": true
            }
          }
        },
        "ITSAppUsesNonExemptEncryption": false,
        "LSApplicationQueriesSchemes": ["itms-apps"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "org.name.pockett3"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "react-native",
          "organization": "originx"
        }
      ],
      "react-native-iap"
    ],
    "extra": {
      "sentry": {},
      "eas": {
        "projectId": "8122915b-9492-4b90-b95c-5adf997dac6d"
      }
    }
  }
}
